{
  "name": "rest",
  "version": "0.0.1",
  "description": "middleware for writing bots",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -x npm run serve",
    "serve": "babel-node index.js -b babel-preset-es2015",
    "test": "mocha --require babel-register tests/*.test.js tests/**/*.test.js --harmony --check-leaks",
    "prepublish": "npm run build",
    "build": "babel . --presets babel-preset-es2015 --out-dir dist --ignore node_modules,.git,dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozzy/rest.git"
  },
  "keywords": [
    "middleware",
    "rest-api",
    "nodejs"
  ],
  "babel": {
    "presets": [
      "stage-3",
      "latest"
    ]
  },
  "author": "Nikita Nikitin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rozzy/rest/issues"
  },
  "homepage": "https://github.com/rozzy/rest#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "install": "^0.8.7",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm": "^4.4.4",
    "should": "^11.2.1"
  }
}
